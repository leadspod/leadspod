{
  "name": "LeadsPod",
  "short_name": "LeadsPod",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Leadspod",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs","storage"]
}
